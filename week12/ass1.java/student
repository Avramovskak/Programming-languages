
public class Student {
	private String name;
	private int indexNo;
	private int age;
	

	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public int getIndexNo() {
		return indexNo;
	}


	public void setIndexNo(int indexNo) {
		this.indexNo = indexNo;
	}


	public int getAge() {
		return age;
	}


	public void setAge(int age) {
		this.age = age;
	}

}
